echo
echo Commands for the EDPS GUI:
echo "   - gui_check: check the status"
echo "   - gui_start: start it"
echo "   - gui_kill: kill it"
echo
echo pipe_install installs the pipeline for a given instrument, including the demo data, if so whished.
echo "   usage: pipe_install -h"
echo

alias cp='cp -i'
alias mv='mv -i'
alias del='rm -i'
alias bck='rm -i *~ .*~ #*# core'
alias clean='find . -name "*~" -exec rm {} \;'
alias dir='ls -ltFGh'
alias ls='ls -G'
alias ll='ls -la'
alias grep='grep --color=auto'
alias pse='ps -U $USER | more'

export PATH=/home/user/bin/:/home/user/.local/bin:$PATH
