echo
echo Commands for the EDPS GUI:
echo    -check_gui: check the status
echo    -start_gui: start it
echo    -kill_gui: kill it
echo

alias cp='cp -i'
alias mv='mv -i'
alias del='rm -i'
alias bck='rm -i *~ .*~ #*# core'
alias clean='find . -name "*~" -exec rm {} \;'
alias dir='ls -ltFGh'
alias ls='ls -G'
alias ll='ls -la'
alias grep='grep --color=auto'
alias pse='ps -U $USER | more'

export PATH=/home/user/bin/:$PATH
